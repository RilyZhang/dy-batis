<?xml version="1.0" encoding="utf-8" ?>
<root>
  <sql id="test1">
    where id = ${id}
    <include ref="test2"/>
  </sql>

  <sql id="test2">
    order by id
  </sql>

  <select id="findUserTest">
    select * from user
    <include ref="test1"/>
  </select>

  <select id="findUser">
    select * from user
    <where>
      <if test="id!==''">id=${id}</if>
      <if test="name!==''">name=${name}</if>
    </where>
    order by id
  </select>

  <insert id="insertUser" precompile="true">
    insert into user set ?
  </insert>

  <insert id="insertUserSet">
    insert into user values (${id}, ${name})
  </insert>
</root>
